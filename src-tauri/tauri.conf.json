  "tauri": {
    "bundle": {
      "active": true,
      "targets": "all",
      "identifier": "co.com.ies.pruebas.tauri-app",
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ],
      "updater": {
        "active": true,
        "pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IDEwMEE2M0ZDNENERTJEQ0IKUldUTExkNU0vR01LRUt3aXM2eXoxZGw5TkJ4elpaUkIyQmhZZzVFVWV5YXVCT2VOMm5yRDN6REkK",
        "windows": {
          "installMode": "passive"
        }
      }
    },
    "security": {
      "csp": null
    },
    "windows": [
      {
        "fullscreen": false,
        "resizable": true,
        "title": "tauri-app",
        "width": 800,
        "height": 600
      }
    ]
  },
  "plugins": {
