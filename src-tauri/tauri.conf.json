{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "tauri-app",
  "version": "0.0.0",
  "identifier": "co.com.ies.pruebas.tauri-app",
  "build": {
    "beforeDevCommand": "",
    "beforeBuildCommand": "",
    "devPath": "../src",
    "distDir": "../src",
    "withGlobalTauri": true
  },

  "package": {
    "productName": "tauri-app",
    "version": "0.0.0"
